{
  "name": "MobilePOS-Tests",
  "version": "1.0.1",
  "description": "WebdriverIO and TypeScript",
  "main": "index.js",
  "scripts": {
    "clean-report": "rimraf allure-results",
    "prettier": "prettier --write --loglevel warn \"**/**/*.ts\"",
    "report:fresh": "npm run clean-report && npm run test ; npm run run-report",
    "report:history": "cp -a ./allure-report/history ./allure-results/",
    "run-report": "allure generate allure-results/ --clean && allure open",
    "test": "npm run prettier && npm run report:history && wdio wdio.conf.js",
    "test:ent1": "wdio wdio.conf.js --suite ent1",
    "test:ent4": "wdio wdio.conf.js --suite ent4",
    "test:adidas": "wdio wdio.conf.js --suite adidas",
    "test:richemont": "wdio wdio.conf.js --suite richemont"
  },
  "author": "ashaporov@gk-software.com",
  "license": "ISC",
  "dependencies": {
    "@slack/webhook": "^5.0.1",
    "@types/chai": "^4.2.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.14.8",
    "@wdio/allure-reporter": "^5.12.1",
    "@wdio/cli": "^5.12.5",
    "@wdio/local-runner": "^5.12.5",
    "@wdio/mocha-framework": "^5.12.1",
    "@wdio/spec-reporter": "^5.12.1",
    "@wdio/sync": "^5.12.3",
    "allure-commandline": "^2.13.0",
    "chai": "^4.2.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.3",
    "webdriverio": "^5.12.5"
  },
  "devDependencies": {
    "@wdio/selenium-standalone-service": "^5.12.1"
  }
}
